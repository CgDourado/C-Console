using static System.Console;

int idade = 0;
int trabalhado = 0;
int todo = 0; 
WriteLine("Qual sua idade?: ");
idade = Convert.ToInt32(ReadLine());
WriteLine("Quantos anos você trabalhou?: ");
trabalhado = Convert.ToInt32(ReadLine());

if (idade >= 65 && trabalhado >= 30)
{
    WriteLine("Você pode se aposentar, aeee!");
}
else if (idade < 65 && trabalhado < 30)
{
    WriteLine("Você não pode se aposentar, Volta a trabalhar!");
}
ReadKey();
